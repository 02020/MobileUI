{
  "compilerOptions": {
    "target": "esnext",
		"moduleResolution": "node",
		"allowJs": true,
		"noEmit": true,
		"strict": true,
		"isolatedModules": true,
		"esModuleInterop": true,
  },
  "include": [
      "./src/**/*.ts",
      "./src/**/*.tsx",
  ],
  "exclude": [
    "./src/__tests__/*"
  ]
}